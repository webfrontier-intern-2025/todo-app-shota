{
    "version": 1,
    "disable_existing_loggers": false,
    "formatters": {
        "defaultFormatter": {
            "format": "[%(asctime)s] [%(levelname)s] [%(name)s] %(message)s",
            "datefmt": "%Y-%m-%d %H:%M:%S"
        }
    },
    "handlers": {
        "consoleHandler": {
            "class": "logging.StreamHandler",
            "level": "DEBUG",
            "formatter": "defaultFormatter",
            "stream": "ext://sys.stdout"
        }
    },
    "loggers": {
        "root": {
            "level": "DEBUG",
            "handlers": [
                "consoleHandler"
            ]
        },
        "sqlalchemy.engine": {
            "level": "INFO",
            "handlers": [
                "consoleHandler"
            ],
            "propagate": false
        },
        "alembic": {
            "level": "INFO",
            "handlers": [
                "consoleHandler"
            ],
            "propagate": false
        },
        "app": {
            "level": "INFO",
            "handlers": [
                "consoleHandler"
            ],
            "propagate": false
        }
    }
}

